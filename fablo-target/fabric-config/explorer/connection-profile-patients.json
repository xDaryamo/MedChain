{
  "name": "fablo-test-network-patients",
  "description": "Connection profile for Hyperledger Explorer in Fablo network",
  "version": "1.0.0",
  "client": {
    "organization": "Patients",
    "tlsEnable": false,
    "enableAuthentication": true,
    "adminCredential": {
      "id": "admin",
      "password": "adminpw"
    },
    "connection": {
      "timeout": {
        "peer": {
          "endorser": "300"
        },
        "orderer": "300"
      }
    }
  },
  "organizations": {
    "Patients": {
      "mspid": "PatientsMSP",
      "adminPrivateKey": {
        "path": "/tmp/crypto/peerOrganizations/patients.medchain.com/users/Admin@patients.medchain.com/msp/keystore/priv-key.pem"
      },
      "peers": [
        "peer0.patients.medchain.com"
      ],
      "signedCert": {
        "path": "/tmp/crypto/peerOrganizations/patients.medchain.com/users/Admin@patients.medchain.com/msp/signcerts/Admin@patients.medchain.com-cert.pem"
      }
    }
  },
  "peers": {
    "peer0.patients.medchain.com": {
      "url": "grpc://peer0.patients.medchain.com:7221",
      "tlsCACerts": {
        "path": "/tmp/crypto/peerOrganizations/patients.medchain.com/msp/tlscacerts/tlsca.patients.medchain.com-cert.pem"
      }
    }
  },
  "channels": {
    "patient-records-channel": {
      "peers": {}
    },
    "identity-channel": {
      "peers": {}
    }
  }
}
