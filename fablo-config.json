{
  "$schema": "https://github.com/hyperledger-labs/fablo/releases/download/1.2.0/schema.json",
  "global": {
    "fabricVersion": "2.5.7",
    "tls": false,
    "engine": "docker",
    "peerDevMode": false,
    "tools": {
      "explorer": true
    }
  },
  "orgs": [
    {
      "organization": {
        "name": "Orderer",
        "domain": "orderer.medchain.com"
      },
      "orderers": [
        {
          "groupName": "medchain-orderergroup",
          "type": "solo",
          "instances": 1
        }
      ]
    },
    {
      "organization": {
        "name": "example",
        "domain": "example.medchain.com"
      },
      "peer": {
        "instances": 1,
        "db": "CouchDb"
      }
    },
    {
      "organization": {
        "name": "OspedaleMaresca",
        "domain": "ospedale-maresca.aslnapoli3.medchain.com"
      },
      "peer": {
        "instances": 1,
        "db": "CouchDb"
      }
    },
    {
      "organization": {
        "name": "MedicinaGeneraleNapoli",
        "domain": "medicina-generale-napoli.medchain.com"
      },
      "peer": {
        "instances": 1,
        "db": "CouchDb"
      }
    },
    {
      "organization": {
        "name": "FarmaciaPetrone",
        "domain": "farmacia-petrone.medchain.com"
      },
      "peer": {
        "instances": 1,
        "db": "CouchDb"
      }
    },
    {
      "organization": {
        "name": "LaboratorioAnalisiCmo",
        "domain": "laboratorio-analisi-cmo.medchain.com"
      },
      "peer": {
        "instances": 1,
        "db": "CouchDb"
      }
    },
    {
      "organization": {
        "name": "Patients",
        "domain": "patients.medchain.com"
      },
      "peer": {
        "instances": 1,
        "db": "CouchDb"
      }
    }
  ],
  "channels": [
    {
      "name": "patient-records-channel",
      "orgs": [
        {
          "name": "example",
          "peers": ["peer0"]
        },
        {
          "name": "OspedaleMaresca",
          "peers": ["peer0"]
        },

        {
          "name": "MedicinaGeneraleNapoli",
          "peers": ["peer0"]
        },

        {
          "name": "Patients",
          "peers": ["peer0"]
        }
      ]
    },
    {
      "name": "prescriptions-channel",
      "orgs": [
        {
          "name": "example",
          "peers": ["peer0"]
        },
        {
          "name": "MedicinaGeneraleNapoli",
          "peers": ["peer0"]
        },

        {
          "name": "FarmaciaPetrone",
          "peers": ["peer0"]
        },

        {
          "name": "OspedaleMaresca",
          "peers": ["peer0"]
        },

        {
          "name": "Patients",
          "peers": ["peer0"]
        }
      ]
    },
    {
      "name": "lab-results-channel",
      "orgs": [
        {
          "name": "example",
          "peers": ["peer0"]
        },
        {
          "name": "OspedaleMaresca",
          "peers": ["peer0"]
        },
        {
          "name": "MedicinaGeneraleNapoli",
          "peers": ["peer0"]
        },

        {
          "name": "LaboratorioAnalisiCmo",
          "peers": ["peer0"]
        },

        {
          "name": "Patients",
          "peers": ["peer0"]
        }
      ]
    },
    {
      "name": "identity-channel",
      "orgs": [
        {
          "name": "example",
          "peers": ["peer0"]
        },
        {
          "name": "OspedaleMaresca",
          "peers": ["peer0"]
        },

        {
          "name": "MedicinaGeneraleNapoli",
          "peers": ["peer0"]
        },

        {
          "name": "FarmaciaPetrone",
          "peers": ["peer0"]
        },

        {
          "name": "LaboratorioAnalisiCmo",
          "peers": ["peer0"]
        },

        {
          "name": "Patients",
          "peers": ["peer0"]
        }
      ]
    }
  ],
  "chaincodes": [
    {
      "name": "patient",
      "version": "0.1",
      "lang": "golang",
      "channel": "identity-channel",
      "directory": "./chaincodes/chaincodes_go/patient"
    },
    {
      "name": "labresults",
      "version": "0.1",
      "lang": "golang",
      "channel": "lab-results-channel",
      "directory": "./chaincodes/chaincodes_go/labresults"
    },
    {
      "name": "practitioner",
      "version": "0.1",
      "lang": "golang",
      "channel": "identity-channel",
      "directory": "./chaincodes/chaincodes_go/practitioner"
    },
    {
      "name": "records",
      "version": "0.1",
      "lang": "golang",
      "channel": "patient-records-channel",
      "directory": "./chaincodes/chaincodes_go/records"
    },
    {
      "name": "organization",
      "version": "0.1",
      "lang": "golang",
      "channel": "identity-channel",
      "directory": "./chaincodes/chaincodes_go/organization"
    },
    {
      "name": "prescription",
      "version": "0.1",
      "lang": "golang",
      "channel": "prescriptions-channel",
      "directory": "./chaincodes/chaincodes_go/prescription"
    },
    {
      "name": "encounter",
      "version": "0.1",
      "lang": "golang",
      "channel": "patient-records-channel",
      "directory": "./chaincodes/chaincodes_go/encounter"
    }
  ]
}
